{
  "settings": {
    "timeout": 1000,
    "provider": "native"
  },
  "relaychain": {
    "chain": "rococo-local",
    "default_command": "tmp/polkadot",
    "default_args": [
      "--no-hardware-benchmarks",
      "-lparachain=debug",
      "--database=paritydb"
    ],
    "nodes": [
      {
        "name": "alice",
        "validator": true
      },
      {
        "name": "bob",
        "validator": true
      }
    ]
  },

  "parachains": [
    {
      "chain": "moonbase-local",
      "id": 1000,
      "genesis": {
        "runtimeGenesis": {
          "patch": {
            "parachainInfo": {
              "parachainId": 1000
            }
          }
        }
      },
      "collator": {
        "args": [
          "--no-hardware-benchmarks",
          "-lparachain=debug",
          "--force-authoring",
          "--database=paritydb"
        ],
        "command": "tmp/moonbeam",
        "name": "alith",
        "ws_port": 33345
      }
    }
  ],
  "types": {
    "Header": {
      "number": "u64",
      "parent_hash": "Hash",
      "post_state": "Hash"
    }
  }
}