{
  "name": "@moonwall/cli",
  "type": "module",
  "version": "5.13.2-dev.4",
  "description": "Testing framework for the Moon family of projects",
  "author": "timbrinded",
  "license": "ISC",
  "homepage": "https://github.com/Moonsong-Labs/moonwall#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Moonsong-Labs/moonwall.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/Moonsong-Labs/moonwall/issues"
  },
  "keywords": [
    "moonwall",
    "moonbeam",
    "moondance",
    "polkadot",
    "kusama",
    "substrate"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=22"
  },
  "files": [
    "dist",
    "bin",
    "*.d.ts",
    "*.mjs"
  ],
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules",
    "build": "zshy",
    "postbuild": "bun generate-types",
    "lint": "bun biome lint ./src",
    "lint:fix": "bun biome lint ./src --apply",
    "fmt": "biome format .",
    "fmt:fix": "biome format . --write",
    "generate-types": "tsc",
    "typecheck": "bun tsc --noEmit",
    "prepublish": "bun run build && bun run generate-types"
  },
  "zshy": {
    "exports": "./src/index.ts",
    "bin": {
      "moonwall": "./src/cmds/entrypoint.ts"
    },
    "cjs": false
  },
  "dependencies": {
    "@acala-network/chopsticks": "^1.2.1",
    "@inquirer/prompts": "^7.8.2",
    "@moonwall/types": "workspace:*",
    "@moonwall/util": "workspace:*",
    "@octokit/rest": "22.0.0",
    "@polkadot/api": "^16.4.4",
    "@polkadot/api-derive": "^16.4.4",
    "@polkadot/keyring": "^13.5.5",
    "@polkadot/rpc-provider": "^16.4.4",
    "@polkadot/types": "^16.4.4",
    "@polkadot/types-codec": "^16.4.4",
    "@polkadot/util": "^13.5.5",
    "@polkadot/util-crypto": "^13.5.5",
    "@types/react": "19.1.10",
    "@types/tmp": "0.2.6",
    "@vitest/ui": "^3.2.4",
    "@zombienet/orchestrator": "0.0.110",
    "@zombienet/utils": "^0.0.29",
    "bottleneck": "2.19.5",
    "cfonts": "^3.3.0",
    "chalk": "^5.4.1",
    "clear": "0.1.0",
    "cli-progress": "3.12.0",
    "colors": "1.4.0",
    "dockerode": "4.0.7",
    "dotenv": "17.2.1",
    "ethers": "^6.15.0",
    "ink": "^6.1.1",
    "jsonc-parser": "3.3.1",
    "minimatch": "10.0.3",
    "pino": "^9.7.0",
    "polkadot-api": "1.16.1",
    "react": "^19.1.1",
    "reflect-metadata": "^0.2.0",
    "semver": "^7.7.2",
    "tiny-invariant": "^1.3.3",
    "tmp": "^0.2.5",
    "viem": "2.33.3",
    "vitest": "3.2.4",
    "web3": "^4.16.0",
    "web3-providers-ws": "4.0.8",
    "ws": "^8.18.3",
    "yaml": "2.8.1",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@types/clear": "^0.1.4",
    "@types/cli-progress": "3.11.6",
    "@types/node": "^24.2.1",
    "@types/semver": "^7.7.0",
    "@types/ws": "^8.18.1",
    "@types/yargs": "^17.0.33",
    "tslib": "^2.8.1",
    "tsx": "^4.20.4",
    "typescript": "5.9.2",
    "zshy": "^0.3.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "bin": {
    "moonwall": "./dist/cmds/entrypoint.js"
  }
}
