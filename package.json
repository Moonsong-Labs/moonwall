{
  "name": "moonwall",
  "type": "module",
  "version": "1.0.0",
  "description": "Testing framework for Substrate-based networks",
  "author": "timbrinded",
  "license": "ISC",
  "homepage": "https://github.com/Moonsong-Labs/moonwall#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Moonsong-Labs/moonwall.git"
  },
  "bugs": {
    "url": "https://github.com/Moonsong-Labs/moonwall/issues"
  },
  "keywords": [
    "moonwall",
    "moonbeam",
    "moondance",
    "polkadot",
    "kusama",
    "substrate",
    "testing"
  ],
  "zshy": {
    "exports": {
      ".": "./src/index.ts"
    },
    "bin": {
      "moonwall": "./src/cli.ts",
      "moondebug": "./src/moondebug.ts"
    },
    "cjs": false
  },
  "engines": {
    "node": ">=20"
  },
  "files": [
    "dist",
    "config_schema.json"
  ],
  "scripts": {
    "build": "zshy",
    "postbuild": "pnpm install",
    "schema": "typescript-json-schema --tsNodeRegister --esModuleInterop -o config_schema.json ./src/types/config.ts MoonwallConfig && pnpm oxfmt --write config_schema.json",
    "clean": "rm -rf dist && rm -rf node_modules && pnpm -r clean ",
    "lint": "oxlint ./src",
    "lint:fix": "oxlint --fix ./src",
    "fmt": "oxfmt --check ./src",
    "fmt:fix": "oxfmt --write ./src",
    "typecheck": "tsgo --noEmit --diagnostics && pnpm -r typecheck",
    "test": "vitest run src/cli/internal/effect/__tests__/",
    "test:e2e": "cd test && pnpm moonwall test dev_multi",
    "start": "cd test && pnpm exec moonwall",
    "display-reports": "pnpm exec vite preview --base __vitest__ --outDir html"
  },
  "dependencies": {
    "@acala-network/chopsticks": "^1.2.5",
    "@ast-grep/napi": "^0.40.5",
    "@effect/cluster": "^0.56.1",
    "@effect/experimental": "^0.58.0",
    "@effect/platform": "^0.94.2",
    "@effect/platform-node": "^0.104.1",
    "@effect/rpc": "^0.73.0",
    "@effect/sql": "^0.49.0",
    "@effect/vitest": "^0.27.0",
    "@effect/workflow": "^0.16.0",
    "@inquirer/prompts": "^8.2.0",
    "@octokit/rest": "22.0.1",
    "@vitest/ui": "^4.0.18",
    "@zombienet/orchestrator": "0.0.114",
    "@zombienet/utils": "^0.0.30",
    "bottleneck": "2.19.5",
    "cfonts": "^3.3.1",
    "chalk": "^5.6.2",
    "clear": "0.1.0",
    "cli-progress": "3.12.0",
    "colors": "1.4.0",
    "dockerode": "4.0.9",
    "dotenv": "17.2.3",
    "effect": "^3.19.15",
    "ethers": "^6.16.0",
    "ink": "^6.5.1",
    "jsonc-parser": "3.3.1",
    "minimatch": "10.1.1",
    "pino": "^10.2.1",
    "pino-pretty": "^13.1.3",
    "polkadot-api": "1.19.2",
    "react": "^19.2.1",
    "reflect-metadata": "^0.2.0",
    "rlp": "3.0.0",
    "semver": "^7.7.3",
    "tiny-invariant": "^1.3.3",
    "tmp": "^0.2.5",
    "viem": "2.44.4",
    "vitest": "4.0.18",
    "web3": "^4.16.0",
    "web3-providers-ws": "4.0.8",
    "ws": "^8.19.0",
    "yaml": "2.8.2",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.8",
    "@effect/language-service": "^0.73.0",
    "@polkadot/api": "16.4.8",
    "@polkadot/api-base": "16.4.8",
    "@polkadot/api-derive": "16.4.8",
    "@polkadot/keyring": "13.5.6",
    "@polkadot/rpc-provider": "16.4.8",
    "@polkadot/types": "16.4.8",
    "@polkadot/types-codec": "16.4.8",
    "@polkadot/util": "13.5.6",
    "@polkadot/util-crypto": "13.5.6",
    "@types/clear": "^0.1.4",
    "@types/cli-progress": "3.11.6",
    "@types/dockerode": "^4.0.1",
    "@types/node": "^25.0.10",
    "@types/react": "19.2.7",
    "@types/semver": "^7.7.1",
    "@types/tmp": "0.2.6",
    "@types/ws": "^8.18.1",
    "@types/yargs": "^17.0.35",
    "@typescript/native-preview": "7.0.0-dev.20260122.4",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "typescript-json-schema": "0.67.1",
    "zshy": "^0.7.0"
  },
  "peerDependencies": {
    "@polkadot/api": "^16.1.0",
    "@polkadot/api-base": "^16.1.0",
    "@polkadot/api-derive": "^16.1.0",
    "@polkadot/keyring": "^13.0.0",
    "@polkadot/rpc-provider": "^16.1.0",
    "@polkadot/types": "^16.1.0",
    "@polkadot/types-codec": "^16.1.0",
    "@polkadot/util": "^13.0.0",
    "@polkadot/util-crypto": "^13.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "cpu-features",
      "esbuild",
      "msgpackr-extract",
      "protobufjs",
      "sqlite3",
      "ssh2",
      "vue-demi"
    ]
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "moonwall": "./dist/cli.js",
    "moondebug": "./dist/moondebug.js"
  }
}
